{
  "name": "react-typescript-boilerplate",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/akihiro-tj/react-typescript-boilerplate.git"
  },
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
